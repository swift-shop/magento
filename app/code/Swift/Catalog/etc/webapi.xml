<?xml version="1.0"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/V1/product/get-list" method="GET">
        <service class="Swift\Catalog\Api\GetProductListInterface" method="execute"/>
        <resources>
            <resource ref="Swift_Catalog::management"/>
        </resources>
    </route>
    <route url="/V1/product/:entityId" method="GET">
        <service class="Swift\Catalog\Api\GEtProductByIdInterface" method="execute"/>
        <resources>
            <resource ref="Swift_Catalog::management"/>
        </resources>
    </route>
</routes>
